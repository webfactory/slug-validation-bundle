{
    "name": "webfactory/slug-validation-bundle",
    "description": "Transparent validation of URL slugs in Symfony applications.",
    "minimum-stability": "stable",
    "license": "MIT",
    "authors": [
        {
            "name": "webfactory GmbH",
            "email": "info@webfactory.de",
            "homepage": "http://www.webfactory.de",
            "role": "Developer"
        }
    ],

    "require": {
        "php": ">= 8.1",
        "symfony/config": "^6.4|^7.0",
        "symfony/dependency-injection": "^6.4|^7.0",
        "symfony/event-dispatcher": "^6.4|^7.0",
        "symfony/http-foundation": "^6.4|^7.0",
        "symfony/http-kernel": "^6.4|^7.0",
        "symfony/routing": "^6.4|^7.0"
    },

    "require-dev": {
        "phpunit/phpunit": "^10.5.58"
    },

    "autoload": {
        "psr-4": {
            "Webfactory\\SlugValidationBundle\\": "src"
        }
    },

    "autoload-dev": {
        "psr-4": {
            "Webfactory\\SlugValidationBundle\\Tests\\": "tests"
        }
    }
}
